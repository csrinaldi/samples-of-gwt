<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Tutorial'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <!--inherits name='com.google.gwt.user.User' /-->

    <!-- We need the JUnit module in the main module,               -->
    <!-- otherwise eclipse complains (Google plugin bug?)           -->
    <inherits name='com.google.gwt.junit.JUnit' />
    
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.activity.Activity" />
    <inherits name="com.google.gwt.place.Place" />
    <!--inherits name="elemental.Elemental"/-->

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <!--inherits name='com.google.gwt.user.theme.standard.Standard' /-->
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
    <!--inherits name='com.github.gwtbootstrap.BootstrapNoResources' /-->
    
    <!--inherits name="org.gwtbootstrap3.GwtBootstrap3"/-->
    <!--inherits name="org.gwtbootstrap3.GwtBootstrap3NoTheme"/-->
    <inherits name="org.gwtbootstrap3.GwtBootstrap3Theme"/>

    <!-- Other module inherits                                      -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.workingflow.gwt.client.Tutorial' />
  
    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
    
    <set-property name="compiler.useSourceMaps" value="true" /> 

    <!-- Specify the paths for translatable code                    -->
    <source path='client' />
    <source path='shared' />
    
    <public path='resources'>
        <include name='style/*.css'/>
        <include name='js/*.js'/>
        <include name='font/*'/>
        <include name='img/*'/>
    </public>

    <stylesheet src='style/blue.css'/>
    <stylesheet src='style/bootstrap.css'/>
    <stylesheet src='style/flexslider.css'/>
    <stylesheet src='style/font-awesome.css'/>
    <stylesheet src='style/lblue.css'/>
    <stylesheet src='style/orange.css'/>
    <stylesheet src='style/prettyPhoto.css'/>
    <stylesheet src='style/purple.css'/>
    <stylesheet src='style/red.css'/>
    <stylesheet src='style/slider.css'/>
    <stylesheet src='style/style.css'/>
    
    

</module>
